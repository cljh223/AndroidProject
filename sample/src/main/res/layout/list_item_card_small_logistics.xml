<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginBottom="@dimen/cardMarginVertical"
        android:layout_marginLeft="@dimen/cardMarginHorizontal"
        android:layout_marginRight="@dimen/cardMarginHorizontal"
        android:layout_marginTop="@dimen/cardMarginVertical"
        app:cardCornerRadius="2dp"
        app:cardElevation="2dp"
        app:cardPreventCornerOverlap="false"
        app:contentPadding="0dp">

        <LinearLayout
            android:id="@+id/logisticsFirstSmallLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/logi_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

            <ImageView
                android:id="@+id/logi_image"
                android:layout_width="150dp"
                android:layout_height="75dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/wh1" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                <TextView
                    android:id="@+id/logi_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="제 1물류창고"
                    android:textStyle="bold"
                    android:textColor="@android:color/black"
                    android:textSize="28sp" />

                <TextView
                    android:id="@+id/logi_usage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="전체이용률: 86%"
                    android:textSize="25sp" />

                </LinearLayout>
            </LinearLayout>

                <TextView
                    android:id="@+id/logi_msg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="10dp"
                    android:text="Total warehouse status : NORMAL\n모든 재고 정보는 어제를 기준으로 합니다"
                    android:textColor="@color/green_teal"
                    android:textSize="16sp" />

            <TableLayout
                android:id="@+id/logi_progress"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:stretchColumns="3">

                <TableRow>

                    <TextView
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="1dp"
                        android:text="상품명"
                        android:textSize="18dp" />

                    <TextView
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="1dp"
                        android:text="적재구역"
                        android:textSize="18dp" />

                    <TextView
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="1dp"
                        android:text="총재고량"
                        android:textSize="18dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="1dp"
                        android:text="재고 적재율"
                        android:textSize="18dp" />
                </TableRow>

                <!--A구역-->
                <TableRow>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="안성탕면"
                        android:textSize="16dp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="A구역"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="338"
                        android:textColor="@android:color/holo_orange_light"
                        android:textSize="16dp" />

                    <ProgressBar
                        android:id="@+id/progressBar1_1"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:paddingRight="4dp"
                        android:layout_span="2"
                        android:max="100"
                        android:progress="1" />
                    <TextView
                        android:id="@+id/index1_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingRight="5dp"
                        android:text="11%"
                        android:textSize="15dp"
                        />

                </TableRow>

                <!--B구역-->
                <TableRow>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="신라면"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="B구역"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:textColor="@android:color/holo_orange_light"
                        android:text="571"/>
                    <ProgressBar
                        android:id="@+id/progressBar1_2"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:paddingRight="4dp"
                        android:layout_span="2"
                        android:max="100"
                        android:progress="1" />
                    <TextView
                        android:id="@+id/index1_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingRight="5dp"
                        android:text="11%"
                        android:textSize="15dp"
                        />
                </TableRow>

                <!--C구역-->
                <TableRow>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="너구리"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="C구역"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:textColor="@android:color/holo_orange_light"
                        android:text="339"/>
                    <ProgressBar
                        android:id="@+id/progressBar1_3"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:paddingRight="4dp"
                        android:layout_span="2"
                        android:max="100"
                        android:progress="1" />
                    <TextView
                        android:id="@+id/index1_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingRight="5dp"
                        android:text="11%"
                        android:textSize="15dp"
                        />
                </TableRow>

                <!--D구역-->
                <TableRow>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="멸치칼국수라면"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="D구역"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:textColor="@android:color/holo_orange_light"
                        android:text="505"/>
                    <ProgressBar
                        android:id="@+id/progressBar1_4"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:paddingRight="4dp"
                        android:layout_span="2"
                        android:max="100"
                        android:progress="1" />
                    <TextView
                        android:id="@+id/index1_4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingRight="5dp"
                        android:text="11%"
                        android:textSize="15dp"
                        />
                </TableRow>

                <!--E구역-->
                <TableRow>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="사리곰탕"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:text="E구역"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="16dp"
                        android:textColor="@android:color/holo_orange_light"
                        android:text="272"/>
                    <ProgressBar
                        android:id="@+id/progressBar1_5"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:paddingRight="4dp"
                        android:layout_span="2"
                        android:max="100"
                        android:progress="1" />
                    <TextView
                        android:id="@+id/index1_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingRight="5dp"
                        android:text="11%"
                        android:textSize="15dp"
                        />
                </TableRow>


            </TableLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/logisticsSecondSmallLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/purchaseSecondBigDetail1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="구매 3" />

            <TextView
                android:id="@+id/purchaseSecondBigDetail2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="구매 3" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/logisticsThirdSmallLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/ThirdigDetail1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="구매 4" />

            <TextView
                android:id="@+id/ThirdBigDetail2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="구매 5" />

        </LinearLayout>
    </android.support.v7.widget.CardView>


</FrameLayout>