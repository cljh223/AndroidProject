                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:textSize="16dp"
                                android:text="적재구역"/>
                            <TextView
                                android:id="@+id/district2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:textSize="16dp"
                                android:text="B구역 / 45㎢"/>
                        </TableRow>
                        <TableRow>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:textSize="16dp"
                                android:text=" 창고이용률  "/>
                            <TextView
